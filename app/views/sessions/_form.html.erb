<%= form_tag sessions_path,id:"login_form", style: 'display: none' do %>
	<div class="field">
		<%= label_tag :email %><br>
		<%= text_field_tag :email, params[:email] %>
	</div>
	<div class="field">
		<%= label_tag :password %><br>
		<%= text_field_tag :password, params[:password] %>
	</div>
	<div class="Actions"><%= submit_tag "Login" %></div>
<% end %>